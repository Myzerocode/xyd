{
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "target": "ES6",
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@/components/*": ["@/components/*"],
        "@/lib/utils/*": ["@/lib/utils/*"],
        "@/components/ui/*": ["@/components/ui/*"],
        "@/components/magicui/*": ["@/components/magicui/*"]
      }
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
