{
  "compilerOptions": {
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "target": "ES6",
    "baseUrl": ".",
    "paths": {
      "@xyd-js/ui": [
        "../xyd-ui"
      ],
      "@xyd-js/content": [
        "../xyd-content"
      ],
      "@xyd-js/components": [
        "../xyd-components"
      ],
      "@xyd-js/core": [
        "../xyd-core"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
