    "engine": {
        "paths": {
            "@components/*": [
                "../../packages/xyd-components/src/*"
            ],
            "@core/*": [
                "../../packages/xyd-core/src/*"
            ]
        }
    },
    "redirects": [
        {
            "source": "index",
            "destination": "docs"
        }
    ]
}