                }
            }
        },
        "anchors": {
            "bottom": [
                {
                    "url": "http://xyd-docs.slack.com",
                    "icon": "docs:slack",
                    "name": "Slack"
                },
                {
                    "url": "https://github.com/xyd-js/examples",
                    "icon": "code",
                    "name": "Example Projects"
                }
            ]
        }
