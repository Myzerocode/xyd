{
    "theme": {
        "name": "picasso",
        "logo": {
            "light": "/public/assets/logo.svg",
            "dark": "/public/assets/logo-dark.svg"
        },
        "favicon": "/public/assets/logo.svg",
        "banner": {
            "content": "**xyd 0.1.0** - Docs platform for future dev",
            "label": "Coming Soon",
            "icon": "sparkles",
            "href": "https://github.com/orgs/livesession/projects/4/views/1"
        },
        "icons": {
            "library": [
                {
                    "name": "lucide",
                    "default": true
                },
                "./icons/iconify.json"
            ]
        }
    },
    "integrations": {
        "search": {
            "orama": true
        },
        "apps": {
            "githubStar": {
                "title": "Star",
                "label": "Show your support! Star us on GitHub ⭐️",
                "href": "https://github.com/livesession/xyd",
                "dataIcon": "octicon-star",
                "dataSize": "large",
                "dataShowCount": true,
                "ariaLabel": "Star livesession/xyd on GitHub"
            }
        }
    },
    "navigation": {
        "header": [
            {
                "title": "Guides",
                "page": "docs",
                "icon": "book"
            },
            {
                "title": "Components",
                "page": "docs/components",
                "icon": "component"
            },
            {
                "title": "Reference",
                "page": "docs/reference",
                "icon": "brackets"
            },
            {
                "title": "Public Roadmap",
                "href": "https://github.com/orgs/livesession/projects/4",
                "float": "right"
            },
            {
                "title": "Demo",
                "href": "https://codesandbox.io/p/github/xyd-js/deploy-samples-codesandbox",
                "float": "right"
            },
            {
                "title": "Feedback",
                "href": "https://github.com/livesession/xyd/discussions",
                "float": "right",
                "component": "Button",
                "props": {
                    "kind": "secondary"
                }
            },
            {
                "href": "https://github.com/livesession/xyd",
                "float": "right",
                "component": "Button",
                "props": {
                    "kind": "secondary",
                    "icon": "docs:github",
                    "theme": "ghost",
                    "size": "xs"
                }
            }
        ],
        "segments": [
            {
                "route": "docs/reference",
                "title": "API",
                "pages": [
                    {
                        "title": "Core",
                        "page": "docs/reference/core"
                    },
                    {
                        "title": "Functions",
                        "page": "docs/reference/functions"
                    },
                    {
                        "title": "Composer",
                        "page": "docs/reference/composer"
                    },
                    {
                        "title": "CLI",
                        "page": "docs/reference/cli"
                    }
                ]
            }
        ],
        "sidebar": [
            {
                "route": "docs",
                "pages": [
                    {
